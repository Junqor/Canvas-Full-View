{
   "manifest_version": 3,
   "name": "Canvas Document Full View",
   "version": "1",
   "description": "Make Canvas files fill the window to faciliatate multitasking on a single screen",
   "action": {
      "default_icon": "images/Canvas.png",
      "default_popup": "popup/popup.html"
   },
   "permissions": ["activeTab", "scripting", "declarativeContent"],
   "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"]
   }]
}